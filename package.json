{
  "name": "core-styles",
  "version": "1.0.0",
  "description": "Core styles I like to use in most of my front end development",
  "main": "index.js",
  "scripts": {
    "lint": "stylelint \"scss/**/*.scss\"",
    "compile-test": "sass --style=compressed test/test.scss css/test.css",
    "compile-main": "sass --style=compressed scss/main.scss css/main.css",
    "test": "npm run lint && npm run compile-test && npm run compile-main"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanpiv/core-styles.git"
  },
  "author": "Ryan Piveteau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryanpiv/core-styles/issues"
  },
  "homepage": "https://github.com/ryanpiv/core-styles#readme",
  "dependencies": {},
  "devDependencies": {
    "@redventures/stylelint-config-rv": "^0.1.0",
    "sass": "^1.39.0",
    "stylelint": "^13.13.1"
  }
}
